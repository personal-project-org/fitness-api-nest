{
  "name": "fitness-api",
  "version": "1.0.1",
  "description": "Development Version - Accounts.",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "deploy": "npm install --legacy-peer-deps && npx prisma migrate deploy && npx prisma generate && npm run start:prod2",
    "start:debug": "nest start --watch --debug",
    "prebuild": "rimraf dist && rimraf node_modules",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run start:dev",
    "start:dev": "export NODE_ENV=development && npm run db:shutdown && npm run db:up && sleep 1 && prisma migrate dev && nest start --watch --debug",
    "timeout": "sleep 2",
    "start:prod": "node dist/src/main",
    "start:prod2": "nest start --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:shutdown": "docker-compose -f docker-compose.yml down",
    "db:up": "docker-compose -f docker-compose.yml up -d"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@badrap/result": "^0.2.13",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^4.5.1",
    "@nestjsi/class-validator": "^0.0.11",
    "@prisma/client": "^4.9.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.10",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "bcrypt": "^5.1.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
