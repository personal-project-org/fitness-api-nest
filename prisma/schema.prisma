// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Food {
  id        String @id @default(uuid())
  name      String
  calories  String
  protein   String
  carbs     String
  fats      String
}

model Exercises {
  id                String @id @default(uuid())
  name              String
  type              String
  body_part         String
  reps              Int[]
  weight            Int[]
}

model Accounts {
  id                    String @id @default(uuid())
  username              String
  calories_goal         String
  protein_goal          String
  carbs_goal            String
  fats_goal            String
}

